{
  "name": "workout-app",
  "version": "1.0.0",
  "description": "App to workout",
  "scripts": {
    "dev": "npm-run-all -p -l watch:*",
    "test": "jest --passWithNoTests",
    "start:client": "node dist-server/index.js",
    "build": "npm-run-all -p build:**",
    "build:client": "parcel build ./client/index.tsx -d ./public -o app.js",
    "build:server": "tsc ./server/index.ts",
    "watch": "npm-run-all -p -l watch:*",
    "watch:server": "ts-node-dev server/index.ts",
    "watch:client": "parcel watch ./client/index.tsx -d ./public -o app.js",
    "watch:browser": "parcel ./public/index.html --port 4100 --open"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "apollo-server-express": "^2.19.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "react": "^16.14.0",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "sass": "^1.28.0",
    "subscriptions-transport-ws": "^0.9.18"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/express": "^4.17.8",
    "@types/mongodb": "^3.6.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/node": "^14.14.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  }
}